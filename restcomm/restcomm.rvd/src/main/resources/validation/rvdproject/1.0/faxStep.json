{
    "$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Fax step",	
	"type": "object",
	"properties": {
		"name":{"type": "string"},
		"kind": {"enum": ["fax"]},
		"label": {"type": "string"},
		"title": {"type": "string"},
		"text": {"type": "string"},
		"next": { "$ref": "rvdproject-schema.json#/nullOrString" },
		"to": {"type": "string"},
		"from": {"type": "string"},
		"method": {"enum": ["GET","POST"]},
		"statusCallback": {"type": "string"},
		
		"iface": {"type": "object"}
	},
	"required": ["name","kind","label","title","text"]	
}

